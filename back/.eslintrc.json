{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "node": true,
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "global": {
        "Automics": "readyOnly",
        "ShareArrayBuffer" : "readOnly",
    },
    "rules": {
        "no-unused-vars" : "warn",
        "no-console" : "off"
    }
}
